# Development Environment Variables
VITE_API_BASE_URL=https://hackathon-dotnet-app-production-36f9.up.railway.app/api
VITE_WS_URL=wss://hackathon-dotnet-app-production-36f9.up.railway.app
VITE_APP_NAME=Wealthify Dev
VITE_APP_VERSION=1.0.0-dev
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# API Configuration
VITE_API_TIMEOUT=10000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Authentication
VITE_JWT_REFRESH_THRESHOLD=300000

# Performance
VITE_PAGINATION_DEFAULT_LIMIT=10
VITE_SEARCH_DEBOUNCE_DELAY=300
VITE_CACHE_MAX_AGE=300000

# Development Tools
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_ENABLE_API_LOGGING=true
